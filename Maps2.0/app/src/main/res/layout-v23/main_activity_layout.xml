<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:background="@drawable/karte_erster_bildschirm"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.eemui.team7.maps20.MainActivity"
    android:weightSum="1">

    <EditText
        android:layout_width="335dp"
        android:layout_height="0dp"
        android:id="@+id/txtFindLocationOne"
        android:background="@android:color/white"
        android:hint="@string/strFindLocationOne"
        android:layout_weight="0.09"
        android:textColorHint="@android:color/widget_edittext_dark"
        android:allowUndo="true"
        android:autoText="true" />
</LinearLayout>
